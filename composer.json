{
  "name": "dotninth/xenogit",
  "description": "Your CLI buddy for instant Git commit messages. ",
  "keywords": [
    "git",
    "commit",
    "message",
    "cli",
    "gpt",
    "gitprompt",
    "gpt-3",
    "gpt3",
    "gpt-4",
    "chatgpt",
    "console",
    "claude",
    "perplexity",
    "gemini"
  ],
  "homepage": "https://github.com/dotninth/xenogit",
  "type": "project",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/dotninth/xenogit/issues",
    "source": "https://github.com/dotninth/xenogit"
  },
  "authors": [
    {
      "name": "Dominik Lokkhart",
      "email": "lokkhart@yard77.dev"
    }
  ],
  "require": {
    "php": "^8.2",
    "guzzlehttp/guzzle": "^7.5",
    "illuminate/http": "^11.5",
    "laravel/prompts": "^0.1.3",
    "nunomaduro/termwind": "^2.0"
  },
  "require-dev": {
    "laravel-zero/framework": "^11.0",
    "laravel/pint": "^1.21",
    "mockery/mockery": "^1.5.1",
    "pestphp/pest": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "bin": [
    "builds/xenogit"
  ]
}
